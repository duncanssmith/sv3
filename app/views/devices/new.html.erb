  </p>
  <p>
    <%= f.label :ram %><br />
    <%= f.text_field :ram %>
  </p>
  <p>
    <%= f.label :bios_date %><br />
    <%= f.date_select :bios_date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', devices_path %>
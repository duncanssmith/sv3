<h1>New device</h1>

<% form_for(@device) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :asset_id %><br />
    <%= f.text_field :asset_id %>
  </p>
  <p>
    <%= f.label :make %><br />
    <%= f.text_field :make %>
  </p>
  <p>
    <%= f.label :model %><br />
    <%= f.text_field :model %>
  </p>
  <p>
    <%= f.label :serial %><br />
    <%= f.text_field :serial %>
  </p>
  <p>
    <%= f.label :network_user_name %><br />
    <%= f.text_field :network_user_name %>
  </p>
  <p>
    <%= f.label :node %><br />
    <%= f.text_field :node %>
  </p>
  <p>
    <%= f.label :os %><br />
    <%= f.text_field :os %>
  </p>
  <p>
    <%= f.label :os_service_level %><br />
    <%= f.text_field :os_service_level %>
  </p>
  <p>
    <%= f.label :chassis %><br />
    <%= f.text_field :chassis %>
  </p>
  <p>
    <%= f.label :domain %><br />
    <%= f.text_field :domain %>
  </p>
  <p>
    <%= f.label :ip %><br />
    <%= f.text_field :ip %>

<% @clients.each do |client| %>
  <%=h client.name %>
<% end %>

<h1>Listing devices</h1>

<% if @devices.blank? %>
				<p>There are no devices listed</p>
<% else %>
<% javascript "pagination" %>

<div id="devices">
<%= render :partial => 'devices' %>
</div>

<% end %>

<br />

<%= link_to 'New device', new_device_path %>
